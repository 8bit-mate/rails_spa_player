<%= turbo_frame_tag "content", data: { turbo_action: "advance" } do %>
  <p style="color: green"><%= notice %></p>

  <%= render @track %>

  <div>
    <%= link_to "Edit this track", edit_track_path(@track) %> |
    <%= link_to "Back to tracks", tracks_path %>

    <%= button_to "Destroy this track", @track, method: :delete, class: "btn btn-danger" %>
  </div>
<% end %>